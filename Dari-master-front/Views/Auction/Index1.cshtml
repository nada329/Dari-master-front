@model IEnumerable<Dari_master_front.Models.AuctionModel>

<p>
    @Html.ActionLink("Create New", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.auction_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.startingprice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.onGoing)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.postdate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.auction_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.startingprice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.onGoing)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.postdate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
        </td>
    </tr>
}

</table>
